{
  "name": "site",
  "version": "1.6.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "gatsby serve",
    "develop": "npm run clean && gatsby develop -o",
    "clean": "rimraf public .cache",
    "build": "npm run clean && run-p build:*",
    "build:site": "gatsby build",
    "build:resume": "mkdirp public && node scripts/export-resume.js src/resume.json public/resume.pdf",
    "test:unit": "jest --runInBand",
    "test:e2e": "CYPRESS_SUPPORT=y npm run build && start-server-and-test 'gatsby serve -H 127.0.0.1' http://127.0.0.1:9000 cy:run",
    "spellcheck": "mdspell --en-us -anxr posts/*/*.md",
    "test": "run-p test:*",
    "lint": "NODE_ENV=test eslint --max-warnings=0 generators scripts src test",
    "cy:run": "(is-ci && cypress run --browser chrome --record) || cypress run --browser chrome --record false",
    "new:post": "yo ./generators/post"
  },
  "dependencies": {
    "gatsby": "^5.14.5",
    "gatsby-plugin-feed": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-less": "^7.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-offline": "^6.14.0",
    "gatsby-plugin-react-helmet": "^6.14.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-remark-copy-linked-files": "^6.14.0",
    "gatsby-remark-draw": "^1.0.16",
    "gatsby-remark-embed-snippet": "^8.14.0",
    "gatsby-remark-embed-video": "^3.2.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^7.14.0",
    "gatsby-remark-images-medium-zoom": "^1.7.0",
    "gatsby-remark-prismjs": "^7.14.0",
    "gatsby-remark-responsive-iframe": "^6.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "less": "^4.3.0",
    "mermaid": "^11.7.0",
    "mermaid.cli": "^0.5.1",
    "moment": "^2.30.1",
    "prismjs": "^1.30.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "github:haegrr/react-helmet#c7a0788",
    "semantic-ui-less": "^2.5.0",
    "semantic-ui-react": "^2.1.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@simonsmith/cypress-image-snapshot": "^9.1.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^30.0.2",
    "babel-preset-gatsby": "^3.14.0",
    "cypress": "^10.11.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-gatsby": "github:haegrr/gatsby-plugin-eslint-config#de1e191",
    "gatsby-cypress": "^3.14.0",
    "generator-license": "^5.7.1",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^4.1.0",
    "jest": "^30.0.3",
    "jsonresume-theme-rocketspacer": "github:haegrr/jsonresume-theme-rocketspacer",
    "markdown-spellcheck": "^1.3.1",
    "mime": "^4.0.7",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^24.11.0",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.6",
    "slugify": "^1.6.4",
    "start-server-and-test": "^2.0.12",
    "wrangler": "^4.22.0",
    "yeoman-generator": "^7.5.1",
    "yo": "^5.1.0"
  },
  "overrides": {
    "react-server-dom-webpack@0.0.0-experimental-c8b778b7f-20220825": {
      "react": "^18.2.0"
    }
  }
}
